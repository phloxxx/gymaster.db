SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
ALTER PROCEDURE [dbo].[SP_AUTHENTICATEUSER]
    @USERNAME VARCHAR(20),
    @PASSWORD VARCHAR(15),
	@USER_TYPE NVARCHAR(10)
AS
BEGIN
    SELECT USER_ID, USER_FNAME, USER_LNAME
    FROM [USER]
    WHERE USERNAME = @USERNAME AND [PASSWORD] = @PASSWORD AND IS_ACTIVE = 1 AND USER_TYPE = @USER_TYPE
END
