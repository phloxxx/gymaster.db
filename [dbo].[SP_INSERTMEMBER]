ALTER PROCEDURE [dbo].[SP_INSERTMEMBER]
    @MEMBER_FNAME VARCHAR(50),
    @MEMBER_LNAME VARCHAR(30),
    @EMAIL VARCHAR(50),
    @PHONE_NUMBER VARCHAR(15),
    @PROGRAM_ID SMALLINT,
    @USER_ID SMALLINT
AS
BEGIN
    INSERT INTO MEMBER (MEMBER_FNAME, MEMBER_LNAME, EMAIL, PHONE_NUMBER, PROGRAM_ID, USER_ID)
    VALUES (@MEMBER_FNAME, @MEMBER_LNAME, @EMAIL, @PHONE_NUMBER, @PROGRAM_ID, @USER_ID)
END
