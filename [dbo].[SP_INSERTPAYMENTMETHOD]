ALTER PROCEDURE [dbo].[SP_INSERTPAYMENTMETHOD]
    @PAY_METHOD VARCHAR(20)
AS
BEGIN
    INSERT INTO PAYMENT (PAY_METHOD)
    VALUES (@PAY_METHOD)
END
