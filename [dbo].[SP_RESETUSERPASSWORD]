USE [GYMASTER]
GO
/****** Object:  StoredProcedure [dbo].[SP_RESETUSERPASSWORD]    Script Date: 09/12/2024 12:48:32 am ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO

ALTER PROCEDURE [dbo].[SP_RESETUSERPASSWORD]
    @USER_ID SMALLINT,
    @NEW_PASSWORD VARCHAR(15)
AS
BEGIN
    UPDATE [USER]
    SET [PASSWORD] = @NEW_PASSWORD
    WHERE USER_ID = @USER_ID
END
