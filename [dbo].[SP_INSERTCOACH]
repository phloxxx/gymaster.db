ALTER PROCEDURE [dbo].[SP_INSERTCOACH]
    @COACH_FNAME VARCHAR(50),
    @COACH_LNAME VARCHAR(30),
    @EMAIL VARCHAR(50),
    @PHONE_NUMBER VARCHAR(15),
    @SPECIALIZATION VARCHAR(550)
AS
BEGIN
    INSERT INTO COACH (COACH_FNAME, COACH_LNAME, EMAIL, PHONE_NUMBER, SPECIALIZATION)
    VALUES (@COACH_FNAME, @COACH_LNAME, @EMAIL, @PHONE_NUMBER, @SPECIALIZATION)
END
