ALTER PROCEDURE [dbo].[SP_ASSIGNCOACHTOPROGRAM]
    @PROGRAM_ID SMALLINT,
    @COACH_ID SMALLINT
AS
BEGIN
    INSERT INTO PROGRAM_COACH (PROGRAM_ID, COACH_ID)
    VALUES (@PROGRAM_ID, @COACH_ID)
END
