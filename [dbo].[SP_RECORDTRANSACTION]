/****** Object:  StoredProcedure [dbo].[SP_RECORDTRANSACTION]    Script Date: 09/12/2024 12:48:06 am ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
ALTER PROCEDURE [dbo].[SP_RECORDTRANSACTION]
    @MEMBER_ID SMALLINT,
    @SUB_ID SMALLINT,
    @PAYMENT_ID SMALLINT
AS
BEGIN
    DECLARE @TRANSACTION_ID SMALLINT

    INSERT INTO [TRANSACTION] (MEMBER_ID, SUB_ID, PAYMENT_ID)
    VALUES (@MEMBER_ID, @SUB_ID, @PAYMENT_ID)

    SET @TRANSACTION_ID = SCOPE_IDENTITY()
END
